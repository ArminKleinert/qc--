let system	    = "@this@"
let architecture    = "@arch@"

let name = 
  try
    let minus = String.index system '-' in
      String.sub system 0 minus
  with Not_found -> "not configured"

let version = 
  try
    let minus = String.index system '-' in
      String.sub system (minus + 1) (String.length system - minus - 1)
  with Not_found -> "not configured"
