#! /usr/bin/scm -l
# 
# $Id$
#
# pp file.ast
#
# Pretty prints a serialized AST as produced by the unpickle routines
# from ASDL generated code. Utilizes the Scheme pretty printer from 
# the Scheme Library for this purpose.

(require 'pretty-print)

(define (pp name)
  (let* ((port (open-input-file name))
         (file (read port)))
          
      (pretty-print file)))

(let ((args (program-arguments)))
    (pp (cadddr args)))
