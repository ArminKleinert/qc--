/* this test shows some white space sensitivity of c-- */

target byteorder big;

f (bits32 i)
{
        switch [1 .. 3] i {
            case 1:         { return (i); }
            case 2 .. 3:      { return (i); }
            /* must be 2 .. 3, not 2..3 */

        }
}
