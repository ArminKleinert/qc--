target
    memsize 8
    byteorder little
    pointersize 32
    wordsize 32
    charset "latin1"
    float "ieee754";

section "data" { align 1; bits8[0::bits32]; }

section "text"
{
    sym@p()
    {
        $r(32)[3::bits5] = 1::bits32;
        $f(32)[1::bits5] = $r(32)[3::bits5];
        $r(32)[3::bits5] = 2::bits32;
        $f(32)[2::bits5] = $r(32)[3::bits5];
        $r(32)[3::bits5] = 3::bits32;
        $f(32)[3::bits5] = $r(32)[3::bits5];
        $r(32)[3::bits5] = ($f(32)[1::bits5]+$f(32)[2::bits5]);
        $r(32)[3::bits5] = ($r(32)[3::bits5]+$f(32)[3::bits5]);
        $f(32)[1::bits5] = $r(32)[3::bits5];
        $r(32)[4::bits5] = 0::bits32;
        $r(32)[3::bits5] = $r(32)[31::bits5];
        $r(32)[3::bits5] = ($r(32)[3::bits5]+$r(32)[4::bits5]);
        $m(32)[$r(32)[3::bits5]] = $f(32)[1::bits5];
        $r(32)[3::bits5] = $m(32)[$r(32)[31::bits5]];
        return ();
        // dataflow node
    }
}
