target byteorder little;


f (bits32 i)
{
        switch [1 .. 3] i {
            case 1::bits8:       { return (i); }
            case 2 .. 3:         { return (i); }
        }

        switch [1 .. 3] i {
            case 0,1::bits8:     { return (i); }
            case 2 .. 3:         { return (i); }
        }
        
        switch [1 .. 3] i {
            case :              { return (i); }
            case 2 .. 3:        { return (i); }
        }
}
